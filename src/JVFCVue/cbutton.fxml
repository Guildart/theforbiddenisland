      </ToggleButton>
      <Button fx:id="butTakeTresor" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#takeTresor" text="Take Tresor">
         <VBox.margin>
            <Insets right="20.0" top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
